{
    "name": "acquia/acquia-cms-starterkit",
    "description": "Acquia CMS starterkit cli command.",
    "license": "GPL-2.0-or-later",
    "type": "project",
    "keywords": [
        "cli",
        "console",
        "drupal",
        "acquia_cms",
        "starterkit"
    ],
    "authors": [
        {
            "name": "Acquia Engineering",
            "homepage": "https://www.acquia.com",
            "role": "Maintainer"
        },
        {
            "name": "Vishal Khode",
            "email": "vishal.khode@acquia.com",
            "role": "Maintainer"
        }
    ],
    "require": {
        "acquia/coding-standards": "^0.8.0",
        "ergebnis/composer-normalize": "^2.25",
        "stecman/symfony-console-completion": "^0.10.1",
        "symfony/config": "^4.1",
        "symfony/console": "^4.1",
        "symfony/dependency-injection": "^4.1",
        "symfony/http-client": "^4.1",
        "symfony/http-kernel": "^4.1",
        "symfony/process": "^4.1",
        "symfony/yaml": "^4.1"
    },
    "require-dev": {
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.2",
        "phpro/grumphp-shim": "^1.5",
        "phpspec/prophecy-phpunit": "^2.0",
        "phpstan/phpstan": "^1.5",
        "phpunit/phpunit": "^9"
    },
    "repositories": {
        "assets": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "AcquiaCMS\\Cli\\": "src/"
        }
    },
    "bin": [
        "bin/acms"
    ],
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpro/grumphp-shim": true,
            "ergebnis/composer-normalize": true
        }
    }
}
